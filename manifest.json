{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "b2b x rainbow texture pack",
  "version": "1.4.1",
  "description": "A browser extension to mod an online game",
  "author": "SmoothDude, joudaALT, Mystic",
  "background": {"service_worker": "background.js"},
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
   "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://miniblox.io/*",
    "https://miniblox.online/*",
    "https://miniblox.org/*",
    "https://raw.githubusercontent.com/*"
  ]
}
